#!/bin/bash
set -e

function frame () {
    echo "frame-$*"
    time eval "frame-$*"
}

frame read\
    --log $1.log\
    --name $1
frame make-graph\
    --contains $1.contains\
    --size $1.size\
    --share $1.share\
    --graph $1.graph
frame deparent\
    --graph $1.graph\
    --dag $1.dag
